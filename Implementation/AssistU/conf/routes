# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page

GET     /                           controllers.Application.index()
#task

GET     /task                       controllers.Application.task()
#login

GET     /login                      controllers.Application.login()
#calendar

GET     /calendar                   controllers.Application.calendar()
#project

GET     /project                    controllers.Application.project()
#inbox

GET     /inbox                      controllers.Application.inbox()
#tips

GET     /tips						controllers.Application.tips()	



# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
GET     /webjars/*file              controllers.WebJarAssets.at(file)
